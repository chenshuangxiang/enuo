//应急通道和医院环境
function fnShowAlbum(albumid){
    $.ajax({
        url:'/api.php?mod=getPhotolist&albumid='+albumid,
        type:"GET",
        dataType:"JSON",
        data:{ },
        success:function(d) {
            var data = d.data;
            var html = [];//'<a href="''" title="文字说明"><img src="thumb_image3.jpg" width="72" height="72"></a>';
            for (var i = 0, len = data.length; i < len; i++) {
                html.push('<a href="/' + data[i].uploadfiles + '" title="' + data[i].intro + '"><img src="/tpl/default/style/images/loading.gif"></a>');
            }
            if(!$('#album').length){
                $('body').append('<div id="album" style="display: none;"></div>');
            }
            $('#album').empty();
            $('#album').append(html.join(''));
            $('#album a').lightBox();
            $('#album a:eq(0)').trigger('click');
        }
    });

}
